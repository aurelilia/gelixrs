exfn print(String a)

exfn printnum(i64 a)

func main() {
    val sub = Sub()
    printnum(sub.a)
    print(sub.t)
    printnum(sub.ok())
    print(sub.nice())
}

class Super {
    val a = 5
    val b = "okay"

    func ok() -> i64 6
    func nice() -> String this.b
}

class Sub ext Super {
    val t = "yes"
}