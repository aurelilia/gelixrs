/*
I am still allocated!
*/

func main() {
    print(a().string)
}

func a() -> A {
    val a = b()
    a()
}

func b() -> (): A {
    val string = "I am still allocated!"
    val a = A(string)
    c(a)
}

func c(a: A) -> (): A {
    (): A -> a
}

class A {
    val string: String
    construct(string) {}
}