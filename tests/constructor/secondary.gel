/*
easy!
All right!
easy!
thanks
good
All right!
good
*/

func main() {
    Test("easy!").simple()
    Test("good", "thanks").simple()
}

class Test {

    val a: String

    construct(a) {
        print(this.a)
        print("All right!")
    }

    construct(b: String, c: String) {
        print(c)
        this.a = b
        print(this.a)
        print("All right!")
    }

    func simple() {
        print(this.a)
    }
}
