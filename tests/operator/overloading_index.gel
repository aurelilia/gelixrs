/*
2
7
*/

import std/ops/+

class IntBox {
    var a = 0
    var b = 0
}

impl IndexGet[bool, i64] for IntBox {
    func index_get(index: bool) -> i64 {
        if (index) this.a
        else this.b
    }
}

impl IndexSet[bool, i64] for IntBox {
    func index_set(index: bool, value: i64) {
        if (index) this.a = value
        else this.b = value
    }
}

func main() {
    var a = new IntBox()
    a(true) = 2
    print(a(true))

    a(false) = 7
    print(a(false))
}