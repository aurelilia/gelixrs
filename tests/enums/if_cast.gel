func main() {
    val a = Krusty:Crab("Hello!")
    val b = Krusty:Patrick(42)

    a.test()
    b.test()
}

enum Krusty {
                               
    func test() {
        if (this is Krusty:Crab) {
            print(this.b)
        } else {
            print("No this is Patrick")
        }
    }

    Crab(val b: String)
    Patrick(val b: i64)
}