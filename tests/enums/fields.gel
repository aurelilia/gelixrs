/*
5
5
2
Hello!
42
true
*/

func main() {
    val a = A:A("Hello!")
    val b = A:B(42)
    val c = A:C(true)

    print(a.a)
    print(b.a)
    print(c.a)

    print(a.print())
    print(b.print())
    print(c.b)
}

enum A {

    var a = 5

    A {
        val b: String

        construct(b) {}

        func print() -> String = this.b
    }

    B {
        val b: i64

        construct(b) {}

        func print() -> String = this.b.to_string()
    }

    C {
        val b: bool

        construct(b) {
            this.a = 2
        }

        func print() -> String = this.b.to_string()
    }
}